{
  "name": "react-patterns",
  "version": "1.0.0",
  "description": "Reusable components library",
  "main": "dist/main.js",
  "scripts": {
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors --collectCoverageFrom=src/**/*.jsx --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/**/*.stories.js",
    "storybook": "start-storybook  -p 9001 -c .storybook",
    "version": "npm run build && git add -f index.js",
    "postversion": "npm run release",
    "release": "git push --tags && git commit -a --amend --no-edit && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junaidrsd/react-patterns.git"
  },
  "keywords": [
    "react",
    "components",
    "patterns"
  ],
  "author": "Junaid Rasheed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/junaidrsd/react-patterns/issues"
  },
  "homepage": "https://github.com/junaidrsd/react-patterns#readme",
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "* || ^16.8.6",
    "react-dom": "* || ^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-centered": "^5.0.11",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  }
}
